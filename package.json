{
  "name": "wdlk",
  "version": "1.0.1",
  "private": true,
  "description": "Woodlike multi package repo",
  "repository": "git@github.com:woodlike/component-library.git",
  "license": "MIT",
  "author": "Mauricio Palma <mauricio.palma@woodlikeocean.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:components": "yarn workspace @wdlk/components build",
    "build:containers": "yarn workspace @wdlk/containers build",
    "build:lib": "yarn workspace styled-factory build",
    "build:store": "yarn workspace @wdlk/store-front build",
    "build:theme": "yarn workspace gatsby-theme-query build",
    "dev:components": "yarn workspace @wdlk/components dev",
    "dev:lib": "yarn workspace theme-query dev",
    "dev:store": "yarn workspace @wdlk/store-front dev",
    "lint:components": "yarn workspace @wdlk/components lint",
    "lint:container": "yarn workspace @wdlk/components lint",
    "lint:lib": "yarn workspace theme-query lint",
    "lint:store": "yarn workspace @wdlk/store-front lint",
    "lint:theme": "yarn workspace gatsby-theme-query lint",
    "test:components": "yarn workspace @wdlk/components test",
    "test:containers": "yarn workspace @wdlk/containers test",
    "test:lib": "yarn workspace theme-query test",
    "test:store": "yarn workspace @wdlk/store-front test",
    "type-check:store": "yarn workspace @wdlk/store-front type-check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "1.19.1",
    "typescript": "3.8.2"
  }
}
